// TryAngleApp Build Configuration
// Add C++ standard library for TensorFlow Lite

OTHER_LDFLAGS = -lc++ -weak_framework CoreAudioTypes -weak_framework UIUtilities
CLANG_CXX_LANGUAGE_STANDARD = gnu++17
CLANG_CXX_LIBRARY = libc++

// iOS 18/Xcode 16 fixes
IPHONEOS_DEPLOYMENT_TARGET = 16.0
ENABLE_USER_SCRIPT_SANDBOXING = NO
